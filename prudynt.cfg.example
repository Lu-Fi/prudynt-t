# Prudynt Settings

rtsp: {
	auth_required: true;
	name: "thingino prudynt";
	username: "thingino";
	password: "thingino";
	port: 554;
};

sensor: {
	model: "gc2053";
	i2c_address: 0x37;
	fps: 24;
};

stream0: {
	format: "H265"
	bitrate: 1000;
	buffers: 2;
	fps: 24;
	gop: 30;
	osd_pos_width: 5;
	osd_pos_height: 5;
};

osd: {
	enabled: 1;
	font_path: "/usr/share/fonts/UbuntuMono-Regular2.ttf";
	font_size: 64;
	font_color: 0xFFFFFFFF;
	font_stroke_enabled: 0;
	font_stroke_size: 64;
	font_stroke_color: 0xFF000000;
	format: "%I:%M:%S %p %m/%d/%Y";
};
